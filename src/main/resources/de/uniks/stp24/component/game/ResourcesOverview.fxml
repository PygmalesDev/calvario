<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<VBox alignment="BOTTOM_CENTER" prefHeight="400.0" prefWidth="300.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane fx:id="SuppliesAnchorPane">
         <children>
            <VBox alignment="CENTER" prefHeight="250.0" prefWidth="100.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Supplies" />
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Overall" />
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Pro Season">
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                        </Text>
                     </children>
                  </HBox>
                  <ListView fx:id="suppliesListView" prefHeight="200.0" prefWidth="200.0" />
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
               <StackPane.margin>
                  <Insets top="10.0" />
               </StackPane.margin>
            </VBox>
            <Button fx:id="hideSuppliesButtons" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0">
               <StackPane.margin>
                  <Insets left="272.0" top="-235.0" />
               </StackPane.margin>
            </Button>
         </children>
      </StackPane>
      <VBox alignment="CENTER">
         <children>
            <Button fx:id="showSuppliesMenuButton" mnemonicParsing="false" onAction="#showSuppliesMenu" prefHeight="25.0" prefWidth="25.0" text="Button">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
            <HBox fx:id="suppliesMenuHBox" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#showCoins" prefHeight="40.0" prefWidth="40.0" />
                  <Button mnemonicParsing="false" onAction="#showPoisons" prefHeight="40.0" prefWidth="40.0" />
                  <Button mnemonicParsing="false" onAction="#showTreasures" prefHeight="40.0" prefWidth="40.0" />
                  <Button mnemonicParsing="false" onAction="#showWeapons" prefHeight="40.0" prefWidth="40.0" />
               </children>
               <VBox.margin>
                  <Insets top="-10.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</VBox>
