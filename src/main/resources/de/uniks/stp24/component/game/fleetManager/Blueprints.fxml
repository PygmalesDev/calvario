<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root maxWidth="290.0" prefWidth="290.0" spacing="5.0" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uniks.stp24.component.game.fleetManager.BlueprintsComponent">
   <HBox>
      <HBox HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <Label fx:id="blueprintTypeLabel" text="Type" />
         <ImageView fx:id="healthImage" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" />
         <Label fx:id="healthLabel" text="Label" />
         <ImageView fx:id="timeImage" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" />
         <Label fx:id="timeLabel" text="Label" />
         <ImageView fx:id="speedImage" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" />
         <Label fx:id="speedLabel" text="Label" />
      </HBox>
   </HBox>
   <HBox fx:id="blueprintHBox" alignment="CENTER_LEFT" spacing="5.0">
      <ImageView fx:id="attackImage" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="attackLabel" text="Label">
         <font>
            <Font size="15.0" />
         </font></Label>
      <ImageView fx:id="defenseImage" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="defenseLabel" minHeight="25.0" minWidth="25.0" text="Label" textAlignment="CENTER">
         <font>
            <Font size="15.0" />
         </font>
         <tooltip>
            <Tooltip fx:id="tooltip" anchorLocation="WINDOW_TOP_RIGHT" autoFix="false" consumeAutoHidingEvents="false" contentDisplay="RIGHT" hideOnEscape="false" />
         </tooltip></Label>
      <ListView prefHeight="30.0" prefWidth="70.0" />
      <Button fx:id="addBlueprintButton" mnemonicParsing="false" onAction="#addBlueprint" prefHeight="25.0" prefWidth="40.0" text="add" />
   </HBox>
   <padding>
      <Insets bottom="25.0" left="15.0" right="10.0" top="20.0" />
   </padding>
</fx:root>
